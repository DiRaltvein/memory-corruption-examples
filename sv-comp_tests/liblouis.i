 fprintf(logFile, "\n");
 fflush(logFile);
 __builtin_va_end(argp);
}
void lou_logEnd(void) {
 if (logFile != ((void *)0) && logFile != stderr) fclose(logFile);
 logFile = ((void *)0);
}
