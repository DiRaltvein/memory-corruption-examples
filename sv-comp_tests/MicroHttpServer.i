void _ParseHeader(int *clisock, char *req_buf) {
  int n;
  int i = 0;
  char *p = req_buf;
  n = recv(*clisock, p, 3, 0);
  if (n == 3) {
    for (i = 3; n > 0; i++) {
      n = recv(*clisock, p + i, 1, 0);
      if (p[i] == ' ') {
        p[i] = '_';
        break;
      }
    }
    if (n > 0)
      i += 1;
    for (; n > 0; i++) {
      n = recv(*clisock, p + i, 1, 0);
      if (p[i] == ' ') {
        p[i] = '\0';
        break;
      }
    }
  }
